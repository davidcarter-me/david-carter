  "scripts": {
    "start": "node scripts/start.js",
    "build": "yarn lint:test && yarn test:ci && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "test:ci": "node scripts/test.js --env=jsdom --ci --no-watch --verbose",
    "lint": "eslint src --fix",
    "lint:test": "eslint src --max-warnings=0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "globals": {
      "DEPLOY_ENV": "test"
    },
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.scss$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|scss|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src",
      "src"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2"
  }
}
